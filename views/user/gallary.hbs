<section class="our-portfolio">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center theratio-align-center">

        <div class="space-40"></div>
        <div class="project-filter-wrapper">
          <ul class="project_filters">
            <li><a
                href="#"
                data-filter="*"
                class="selected btn-details"
              >All<span class="filter-count"></span></a></li>
            <li><a
                class="btn-details"
                href="#"
                data-filter=".FaucetsShowers"
              >Faucets&Showers<span class="filter-count"></span></a></li>
            <li><a
                class="btn-details"
                href="#"
                data-filter=".Sanitaryware"
              >Sanitaryware<span class="filter-count"></span></a></li>
            <li><a
                class="btn-details"
                href="#"
                data-filter=".Closet"
              >Closet<span class="filter-count"></span></a></li>
            <li><a class="btn-details" href="#" data-filter=".WaterTanks">Water
                Tanks<span class="filter-count"></span></a></li>
            <li><a class="btn-details" href="#" data-filter=".WaterTanks">
                Kitchen Items&Stores<span class="filter-count"></span></a></li>
            <li><a
                class="btn-details"
                href="#"
                data-filter=".Chimney"
              >Chimney<span class="filter-count"></span></a></li>
          </ul>
          <div class="projects-grid pf_3_cols style-1 img-scale w-auto">
            <div class="grid-sizer"></div>

            {{#each products}}
              <div class="project-item {{this.category}}">
                <div class="projects-box">
                  <div class="projects-thumbnail">
                    <a >
                      <img src="/gallery-image/{{this._id}}.jpg" alt="" />
                    </a>
               
                  </div>
                  {{!-- <div class="portfolio-info">
                    <div class="portfolio-info-inner">
                      <h5><a
                          class="title-link"
                          
                        >{{this.name}}</a></h5>
                      <p class="portfolio-cates"><a
                          href="#"
                        >{{this.category}}</a></p>
                    </div>
              
                  </div> --}}
                </div>
              </div>
            {{/each}}

          </div>

        </div>
      </div>
    </div>
  </div>
</section>